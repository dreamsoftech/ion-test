<script type="text/javascript">
  setInterval(function(){
  	
    $.getJSON("http://uploading-test.herokuapp.com/products",function(result){
    	$("#product-thumbnails").html("");
      $.each(result, function(i, field){
        $("#product-thumbnails").append("<image class='img-thumbnail' src='" + field + "'/>");
      });
    });
  }, 3000);
</script>